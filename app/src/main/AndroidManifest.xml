    package="com.example.wlx65.androidhomework">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.wlx65.androidhomework.Main"
            android:label="Android作业集">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.wlx65.androidhomework.Ex1"
            android:label="作业1" />
        <activity
            android:name="com.example.wlx65.androidhomework.Ex2_1"
            android:label="作业2-1" />
        <activity android:name="com.example.wlx65.androidhomework.Ex2_2"
            android:label="作业2-2"/>
    </application>

</manifest>